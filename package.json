{
  "name": "react-zmage",
  "version": "0.8.5-beta.37",
  "description": "A scalable image container",
  "keywords": [
    "react",
    "image",
    "zmage",
    "zoom",
    "zoomable",
    "slidershow"
  ],
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Caldis/react-zmage.git"
  },
  "author": "caldis",
  "license": "MIT",
  "homepage": "https://zmage.caldis.me",
  "scripts": {
    "dev": "webpack serve --config ./scripts/webpack.web.dev.config.js --progress --profile --stats-error-details",
    "build": "npm run build:lib && npm run build:ssr && npm run build:web",
    "build:lib": "webpack --config ./scripts/webpack.lib.config.js --progress --profile --stats-error-details",
    "build:ssr": "BUILD_MODE=SSR webpack --config ./scripts/webpack.lib.config.js --progress --profile --stats-error-details",
    "build:web": "webpack --config ./scripts/webpack.web.pod.config.js --progress --profile --stats-error-details"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react-use": "^17.3.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.0",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "less": "^4.1.1",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss-loader": "^6.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "source-map-loader": "^3.0.0",
    "source-map-support": "^0.5.19",
    "style-loader": "^3.3.1",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "transfer-webpack-plugin": "^0.1.4",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.7.1",
    "webpack-merge": "^5.7.3"
  }
