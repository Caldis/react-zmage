    "@types/react-dom": "^17.0.3",
    "@vitejs/plugin-react": "^4.3.2",
    "less": "^4.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^4.2.4",
    "vite": "^5.4.10"
  }
}
